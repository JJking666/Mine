.linear_button {
  background-image: linear-gradient(to right bottom, #b28ff2, #00ACD7, #b892fb);
}
.linear_back {
  background-image: linear-gradient(to bottom, #51BDE7, #C6AEF1);
}
#filterImg {
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  object-fit: cover;
  -webkit-filter: blur(8rpx);
          filter: blur(8rpx);
}
.radius-button {
  border: 1px solid transparent;
  border-radius: 4vh;
}
.box_shadow {
  box-shadow: 6rpx 6rpx 2rpx #b1eaf5;
}
.create {
  border: 1px solid transparent;
  border-radius: 50%;
  position: fixed;
  right: 4vw;
  bottom: 20vh;
  width: 15vw;
  height: 15vw;
  background-color: #51BDE7;
  padding: 2vw 2vw;
  box-sizing: border-box;
  border: 1rpx solid transparent;
  box-shadow: 0 0 55rpx 5rpx #4fafef;
}
.create image {
  width: 11vw;
  height: 11vw;
  size: 100%;
}
.buttonStyle1 {
  background-image: linear-gradient(to right bottom, #7867cc, #b7bafb);
}
.buttonStyle2 {

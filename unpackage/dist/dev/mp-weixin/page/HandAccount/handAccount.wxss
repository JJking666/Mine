.radius-button {
  border: 1px solid transparent;
  border-radius: 2vh;
}
.create {
  border: 1px solid transparent;
  border-radius: 50%;
  position: fixed;
  right: 4vw;
  bottom: 20vh;
  width: 15vw;
  height: 15vw;
  background-color: #a7dab9;
  padding: 2vw 2vw;
  box-sizing: border-box;
  border: 1rpx solid #A5A5A5;
}
.create image {
  width: 11vw;
  height: 11vw;
  size: 100%;
}
editor {
  height: 71vh !important;
}
.uni-app--showleftwindow + .uni-tabbar-bottom {
  display: none;
}
#nothing {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  height: 100vh;
  width: 100vw;
}
#nothing > text {
  z-index: 9;
  font-size: larger;
  font-weight: 600;
}
#nothing > image {
  margin-top: 1vh;
  z-index: 9;
  width: 7vh;
  height: 7vh;
  object-fit: cover;
}
#nothing > image:nth-of-type(1) {
  position: absolute;
  margin-top: 0vh;
  z-index: 0;
  top: 0;
  left: 0;
  height: inherit;
  width: inherit;
  object-fit: cover;
}

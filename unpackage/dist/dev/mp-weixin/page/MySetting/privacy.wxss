@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.radius-button {
  border: 1px solid transparent;
  border-radius: 2vh;
}
.create {
  border: 1px solid transparent;
  border-radius: 50%;
  position: fixed;
  right: 4vw;
  bottom: 20vh;
  width: 15vw;
  height: 15vw;
  background-color: #a7dab9;
  padding: 2vw 2vw;
  box-sizing: border-box;
  border: 1rpx solid #A5A5A5;
}
.create image {
  width: 11vw;
  height: 11vw;
  size: 100%;
}
.privacy {
  width: 100vw;
  height: 100vh;
  padding: 2vh 5vw;
  box-sizing: border-box;
}
.privacy .content1 {
  box-sizing: border-box;
  margin-top: 2vh;
  width: 90vw;
  height: -webkit-fit-content;
  height: fit-content;
  background-color: #e8e8e8;
  border: 1rpx solid transparent;
  border-radius: 12rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding: 2vh 5vw;
}
.privacy .content1 .content1-item {
  height: -webkit-fit-content;
  height: fit-content;
  width: 80vw;
  padding: 1vh 0;
  border-bottom: 1rpx solid #dedede;
  box-sizing: border-box;
}
.privacy .content1 .content1-item text {
  line-height: 4vh;
  display: inline-block;
  font-size: 28rpx;
}
.privacy .content1 .content1-item #sw {
  float: right;
  line-height: 4vh;
  display: inline-block;
  -webkit-transform: scaleX(1.2) scaleY(0.9);
          transform: scaleX(1.2) scaleY(0.9);
  margin-top: -0.2vh;
}
.privacy .content2 {
  box-sizing: border-box;
  margin-top: 2vh;
  width: 90vw;
  height: -webkit-fit-content;
  height: fit-content;
  background-color: #e8e8e8;
  border: 1rpx solid transparent;
  border-radius: 12rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding: 2vh 5vw;
}
.privacy .content2 .border {
  width: 80vw;
  height: 1vh;
  position: absolute;
  top: 5vh;
  left: 5vw;
  border-bottom: 1rpx solid #d1ddde;
}

